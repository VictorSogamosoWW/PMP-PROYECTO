{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "types": ["node", "express", "multer"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}